@model ShopCartViewModel
    <div class="container">
        @foreach (var el in Model.shopCart.ListShopItems)
        {
            <div class="alert alert-warning mt-3">
                <b>Автомобиль:</b> @el.CarItem.Name <br />
                <b>Цена:</b> @el.Price.ToString("c") <br />
            </div>
        }
        <hr />
        <p><a class="btn btn-danger" asp-controller="Order" asp-action="Checkout">Оплатить</a> </p>
    </div>